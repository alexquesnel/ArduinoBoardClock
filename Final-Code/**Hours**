int a = 1;
int b = 2;
int c = 3;
int d = 4; 
int e = 5;
int f = 10;
int g = 11;
void setup() {
  // put your setup code here, to run once:
pinMode (a, OUTPUT);
pinMode (b, OUTPUT);
pinMode (c, OUTPUT);
pinMode (d, OUTPUT);
pinMode (e, OUTPUT);
pinMode (f, OUTPUT);
pinMode (g, OUTPUT);
}

void loop() {
  // put your main code here, to run repeatedly:
  
   digitalWrite(b, HIGH);
   delay(60000);
   digitalWrite(b, LOW);  
   digitalWrite(c, HIGH); 
   delay(60000);
   digitalWrite(b, HIGH);
   delay(60000);
   digitalWrite(b, LOW);
   digitalWrite(c, LOW);
   digitalWrite(d, HIGH); 
   delay(60000);
   digitalWrite(b, HIGH);
   delay(60000);
   digitalWrite(b, LOW);
   digitalWrite(c, HIGH);
   delay(60000);
   digitalWrite(b, HIGH);
   delay(60000);
   digitalWrite(d, LOW);
   digitalWrite(b, LOW);
   digitalWrite(c, LOW);
   digitalWrite(e, HIGH); 
   delay(60000);
   digitalWrite(b, HIGH);
   delay(60000);
   digitalWrite(e, LOW);

   

}
